  <string name="enroll2sv_welcome_page_title" msgid="7315749770561704214">"Kasdien užgrobiama tūkstančiai paskyrų internete."</string>
  <string name="enroll2sv_welcome_page_details" msgid="2035373304224001569">"Naudojant patvirtinimą dviem veiksmais „Google“ paskyra apsaugoma net tuo atveju, jei pavagiamas jūsų slaptažodis."</string>
  <string name="enroll2sv_intro_page_enter_password_title" msgid="3845725384905471573">"Kad prisijungtumėte, turėsite įvesti slaptažodį."</string>
  <string name="enroll2sv_intro_page_enter_password_details" msgid="6081950394681853992">"Tačiau, jei naudojate naują kompiuterį ar įrenginį, „Google“ paprašys specialaus kodo, kad galėtų įsitikinti, jog tai iš tiesų esate jūs."</string>
  <string name="enroll2sv_intro_page_enter_code_title" msgid="2526771905197548496">"Jei naudojate naują kompiuterį, įveskite kodą iš telefono."</string>
  <string name="enroll2sv_intro_page_enter_code_details" msgid="4247727206761654112">"Ši programa sugeneruos kodą, kurio reikės prisijungiant."</string>
  <string name="enroll2sv_intro_page_verify_device_title" msgid="6058390532717821203">"Taip prisijungsite."</string>
  <string name="enroll2sv_intro_page_verify_device_details" msgid="3947649573696617810">"Taip pat galite pasirinkti įsiminti kompiuterį, kad „Google“ vėl neprašytų kodo."</string>
