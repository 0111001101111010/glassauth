<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  <string name="enroll2sv_welcome_page_title" msgid="7315749770561704214">"Katru dienu tiek nozagti tūkstošiem tiešsaistes kontu."</string>
  <string name="enroll2sv_welcome_page_details" msgid="2035373304224001569">"Divpakāpju verifikācija aizsargā jūsu Google kontu arī tad, ja jūsu parole ir nozagta."</string>
  <string name="enroll2sv_intro_page_enter_password_title" msgid="3845725384905471573">"Lai pierakstītos, ievadiet paroli."</string>
  <string name="enroll2sv_intro_page_enter_password_details" msgid="6081950394681853992">"Ja izmantosiet jaunu datoru vai ierīci, Google pieprasīs ievadīt īpašu kodu, lai pārliecinātos, vai tas patiesi esat jūs."</string>
  <string name="enroll2sv_intro_page_enter_code_title" msgid="2526771905197548496">"Jaunajā datorā ievadiet kodu no sava tālruņa."</string>
  <string name="enroll2sv_intro_page_enter_code_details" msgid="4247727206761654112">"Šī lietotne ģenerēs kodu, kas nepieciešams, lai pierakstītos."</string>
  <string name="enroll2sv_intro_page_verify_device_title" msgid="6058390532717821203">"Esat pierakstījies."</string>
  <string name="enroll2sv_intro_page_verify_device_details" msgid="3947649573696617810">"Jūs varat izvēlēties iegaumēt datoru, un Google vairs nepieprasīs ievadīt kodu."</string>
  <string name="button_next" msgid="2353761571069251223">"Tālāk"</string>
  <string name="button_back" msgid="2814588052595466630">"Atpakaļ"</string>
