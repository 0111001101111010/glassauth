  <string name="enroll2sv_intro_page_verify_device_details" msgid="3947649573696617810">"Μπορείτε να επιλέξετε την απομνημόνευση του υπολογιστή και δεν θα σας ζητηθεί ξανά κωδικός για αυτόν από την Google."</string>
  <string name="button_next" msgid="2353761571069251223">"Επόμενο"</string>
